<div>
	<%= form_for @food do |f| %>
		<div>
			<%= f.label :name %>
			<%= f.text_field :name %>
		</div>
		<div>
			<%= f.label :cuisine %>
			<%= f.text_field :cuisine %>
		</div>
		<div>
			<%= f.label :price %>
			<%= f.number_field :price %>
		</div>
		<div>
			<%= f.label :location %>
			<%= f.text_field :location %>
		</div>
		<div class="submit-field">
			<%= f.submit %>
		</div>
		<% end %>	
</div>