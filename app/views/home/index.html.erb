<h1>Home#index</h1>

<div>
	<%= form_for :logins, url: logins_path, method: :create do |f| %>
		<div>
			<%= f.label :email %>
			<%= f.text_field :email, type: "email" %>
		</div>
		<div>
			<%= f.label :password %>
			<%= f.text_field :password %>
		</div>
		<div class="submit-field">
			<%= f.submit %>
		</div>
	<% end %>	
</div>

<p><%= link_to "Sign Up", new_user_path %></p>

<p>test</p>

